<h1>Contacts List</h1>
<hr>
<hr>
<div>
  <button id = "get-all">Get all contacts</button>
</div>
<div class = "name-search">
  Search:<input type="text" name="search-name" id = "name-search-box">
  <button id = "name-search-button">Search Last Name</button>
</div>
<div class = "id-search">
  Search:<input type="text" name="search-id" id = "id-search-box">
  <button id = "id-search-button">Search For ID</button>
</div>
<div class = "delete-user">
  Search:<input type="text" name="delete-user" id = "delete-search-box">
  <button id = "delete-button">Delete ID</button>
</div>



<div class = "contact-form">
  <label>First Name:</label>
  <input type="text" name="first-name" id="first-name">
  <label>Last Name:</label>
  <input type="text" name="last-name" id="last-name">
  <label>Email:</label>
  <input type="text" name="email" id="email">
  <label>Phone:</label>
  <input type="text" name="phone" id="phone">
  <button id = "add-contact">Add contact</button>

</div>